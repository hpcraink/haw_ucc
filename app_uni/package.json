{
  "name": "frontend",
  "version": "1.0.0",
  "description": "bwUniCluster stats",
  "main": "main.js",
  "bin": {
    "frontend": "app.js"
  },
  "scripts": {
    "clean": "rm -rf src/app/users/*",
    "clean:all": "find * -maxdepth 0 ! -name package.json -not -name scripts -not -name saves -print0 | xargs -0 rm -rf --",
    "clean:js": "find src/ -name '*.js' -type f -exec rm {} -f \\;",
    "clean:js.map": "find src/ -name '*.js.map' -type f -exec rm {} -f \\;",
    "clean:json": "find src/app/ -name '*.json' -type f -exec rm {} -f \\;",
    "clean:src": "npm run clean:js && npm run clean:js.map && npm run clean:json && rm -f src/main.aot.ts",
    "copy:new": "rsync -uavh --exclude package.json --exclude node_modules --exclude aot_files/htppwd ../app_angular/* .",
    "copy:app": "rsync -uavh --delete-excluded ../app_angular/src/app/* src/app/",
    "cp:aot": "cp aot_files/index.html aot_files/favicon.ico public/ && cp aot_files/main.aot.ts src/",
    "make:new": "npm run clean:all && npm run copy:new",
    "make": "npm run clean:all && npm run copy:new && npm run clean",
    "cp:css": "cp public/css/styles.css src/",
    "build:css": "webpack --config webpack/css.config.js && npm run cp:css",
    "create:new": "./scripts/create.sh -b",
    "watch": "ng serve --aot --watch",
    "update": "./scripts/update.sh"
  },
  "author": "Alexandru Saramet",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/cli": "^7.3.9",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ngtools/webpack": "^7.3.9",
    "@types/core-js": "^2.5.2",
    "angular-google-charts": "^0.1.6",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "rxjs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "core-js": "^3.1.4"
  }
}
