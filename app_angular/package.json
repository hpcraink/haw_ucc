{
  "name": "frontend",
  "version": "1.0.0",
  "description": "bwUniCluster stats",
  "main": "main.js",
  "bin": {
    "frontend": "app.js"
  },
  "scripts": {
    "sync:htpacc": "rsync -uavh aot_files/htppwd/htaccess asaramet@rzlx0900.hs-esslingen.de:~/.public_html/auslastung/.htaccess",
    "sync:htpwd": "rsync -uavh aot_files/htppwd/htpasswd asaramet@rzlx0900.hs-esslingen.de:~/.htpasswd",
    "sync:htpall": "npm run sync:htpacc && npm run sync:htpacc",
    "clean:js": "find src/ -name '*.js' -type f -exec rm {} -f \\;",
    "clean:js.map": "find src/ -name '*.js.map' -type f -exec rm {} -f \\;",
    "clean:json": "find src/app/ -name '*.json' -type f -exec rm {} -f \\;",
    "clean:src": "npm run clean:js && npm run clean:js.map && npm run clean:json && rm -f src/main.aot.ts",
    "clean:2017": "find src/app/2017/ -name [1-9] -exec rm {} -rf \\; -o -name 1[0-2] -exec rm {} -rf \\;",
    "clean:2018": "find src/app/2018/ -name [1-9] -exec rm {} -rf \\; -o -name 1[0-2] -exec rm {} -rf \\;",
    "clean:2019": "find src/app/2019/ -name [1-9] -exec rm {} -rf \\; -o -name 1[0-2] -exec rm {} -rf \\;",
    "clean:public": "rm -rf public/*",
    "clean": "npm run clean:src && npm run clean:public",
    "cp:aot": "cp aot_files/index.html aot_files/favicon.ico public/ && cp aot_files/main.aot.ts src/",
    "cp:css": "cp public/css/styles.css src/",
    "cp:htpacc": "cp aot_files/htppwd/htaccess public/.htaccess",
    "build": "npm run clean && npm run cp:aot && ngc -p tsconfig-prod-aot.json",
    "build:css": "webpack --config webpack/css.config.js && npm run cp:css",
    "prod": "npm run build && npm run build:css && webpack --config webpack/aot.config.js",
    "compile": "[[ ! -d public ]] && mkdir public; npm run prod && npm run cp:htpacc && npm run clean:src",
    "sync": "rsync -uavh public/ asaramet@rzlx0900.hs-esslingen.de:~/.public_html/auslastung/ --delete-excluded",
    "save": "[[ -d public ]] && [[ -d ./prod_work ]] && rm ./prod_work -rf;  mv public prod_work || echo 'compile first!!!'",
    "watch": "ng serve --aot --watch"
  },
  "author": "Alexandru Saramet",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular/animations": "^7.2.4",
    "@angular/cdk": "^7.3.2",
    "@angular/cli": "^7.3.1",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/material": "^7.3.2",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@ngtools/webpack": "^7.3.1",
    "angular-google-charts": "^0.1.3",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "rxjs": "^6.4.0",
    "sass-loader": "^7.1.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "zone.js": "^0.8.29"
  },
  "dependencies": {}
}
